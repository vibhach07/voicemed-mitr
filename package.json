{
  "name": "voice-med-mitr",
  "version": "1.0.0",
  "description": "Voice-first AI health companion for visually impaired users",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": ["accessibility", "healthcare", "voice-ai", "assistive-technology"],
  "author": "Voice-Med-Mitr Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "fast-check": "^3.15.0"
  },
  "dependencies": {
    "uuid": "^9.0.0",
    "@types/uuid": "^9.0.0"
  }
}